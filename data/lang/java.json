{
  "extension": ".java",
  "preImported": ["java.lang"],
  "templates": [
    {
      "name": "Spring Entity",
      "templateFile": "entity.template",
      "engine": "entity"
    },
    {
      "name": "Spring Repository",
      "templateFile": "repository.template",
      "engine": "repository"
    },
    {
      "name": "Spring REST Controller",
      "templateFile": "rest_controller.template",
      "engine": "rest_controller"
    },
    {
      "name": "My Framework Rest Controller",
      "templateFile": "java/rest_controller_weeby.template",
      "engine": "rest_controller"
    }
  ],
  "traductions": {
    "{": "{",
    "}": "}",
    "end_line": ";",
    ".": ".",
    "/*": "/*",
    "*/": "*/",
    "package": "package",
    "import": "import",
    "return": "return",
    "class": "class",
    "this": "this",
    "public": "public",
    "private": "private",
    "void": "void",
    "final": "final",
    "attribute_name_prefix": "",
    "annotation_start": "@",
    "annotation_end": "",
    "rest_annotation_import": "org.springframework.web.bind.annotation.*",
    "post_annotation": "PostMapping",
    "get_annotation": "GetMapping",
    "put_annotation": "PutMapping",
    "delete_annotation": "DeleteMapping",
    "rest_controller_annotation": "RestController",
    "path_variable_annotation": "PathVariable",
    "request_body_annotation": "RequestBody",
    "entity_annotation": "Entity",
    "entity_import_annotation": "jakarta.persistence.Entity",
    "entity_id_annotation": "Id",
    "entity_id_import_annotation": "jakarta.persistence.Id",
    "repository_extends": "JpaRepository",
    "repository_import_extends": "org.springframework.data.jpa.repository.JpaRepository",
    "repository_annotation": "Repository",
    "repository_import_annotation": "org.springframework.stereotype.Repository",
    "interface": "interface",
    "extends": "extends"

  },
  "mappings": [
    {
      "mappingType": "Integer",
      "equivalentType": "int",
      "importPackage": ""
    },
    {
      "mappingType": "String",
      "equivalentType": "String",
      "importPackage": "java.lang.String"
    },
    {
      "mappingType": "Long",
      "equivalentType": "long",
      "importPackage": ""
    },
    {
      "mappingType": "Float",
      "equivalentType": "float",
      "importPackage": ""
    },
    {
      "mappingType": "Double",
      "equivalentType": "double",
      "importPackage": ""
    },
    {
      "mappingType": "BigDecimal",
      "equivalentType": "BigDecimal",
      "importPackage": "java.math.BigDecimal"
    },
    {
      "mappingType": "LocalDate",
      "equivalentType": "LocalDate",
      "importPackage": "java.time.LocalDate"
    },
    {
      "mappingType": "LocalTime",
      "equivalentType": "LocalTime",
      "importPackage": "java.time.LocalTime"
    },
    {
      "mappingType": "LocalDateTime",
      "equivalentType": "LocalDateTime",
      "importPackage": "java.time.LocalDateTime"
    },
    {
      "mappingType": "Byte",
      "equivalentType": "byte",
      "importPackage": ""
    },
    {
      "mappingType": "Boolean",
      "equivalentType": "boolean",
      "importPackage": ""
    }
  ]
}
